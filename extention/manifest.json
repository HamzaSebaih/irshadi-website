{
    "name": "Irshadi Platform",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "images/icon-16.png"
        }
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["https://odusplus-ss.kau.edu.sa/PROD/ywsksinf.P_Display_All_Info"]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}